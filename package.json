{
  "name": "@fable/gl",
  "version": "4.10.0-beta.21",
  "description": "Creates a WebGL context without a window",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "browser": "browser_index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "test": "standard | snazzy && tape test/*.js | faucet",
    "build": "node-gyp build",
    "rebuild": "node-gyp rebuild --verbose",
    "install": "node-pre-gyp install || true"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.7",
    "bindings": "^1.5.0",
    "bit-twiddle": "^1.0.2",
    "glsl-tokenizer": "^2.0.2",
    "nan": "^2.15.0",
    "node-abi": "^2.30.1",
    "node-gyp": "^7.1.2",
    "prebuild-install": "^5.3.6"
  },
  "devDependencies": {
    "angle-normals": "^1.0.0",
    "bunny": "^1.0.1",
    "faucet": "0.0.1",
    "gl-conformance": "^2.0.9",
    "log4js": "^6.3.0",
    "prebuild": "^10.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "tape": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fablestudios/headless-gl.git"
  },
  "keywords": [
    "webgl",
    "opengl",
    "gl",
    "headless",
    "server",
    "gpgpu"
  ],
  "author": "Mikola Lysenko",
  "contributors": [
    "Fable (https://fable.app)"
  ],
  "license": "BSD-2-Clause",
  "gypfile": true,
  "binary": {
    "module_name": "webgl",
    "module_path": "./build/Release",
    "host": "https://github.com/fablestudios/headless-gl/releases/download/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "remote_path": "v{version}"
  }
}